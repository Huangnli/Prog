#CÃ³digo de soma
.data
    a: .word 10
    b: .word 5

.text
addi $v0, $zero, 5	# primeira leitura (a)
syscall

add $t0, $zero, $v0	# armazena a em t0

addi $v0, $zero, 5	# segunda leitura (b)
syscall

add $t1, $zero, $v0	# guarda b em t1

add $a0, $t0, $t1	# faz a soma de a + b, guarda em a0  

addi $v0, $zero, 1	# printf a0
syscall

